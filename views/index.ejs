    <script>
        function hoverOverLike(e) {
            const likeButton = e.target
            likeButton.classList.remove('far')
            likeButton.classList.add('fas')
            likeButton.style.color = '#16e16e'
        }
        function hoverOutOfLike(e) {
            const likeButton = e.target
            likeButton.classList.add('far')
            likeButton.classList.remove('fas')
            likeButton.style.color = '#fff'
        }
        function toggleLyrics(e) {
            const lyricsButton = e.target
            if (lyricsButton.classList.contains('greenButton')) {
                document.getElementById('mainScreen').style.display = 'block'
                document.getElementById('lyricsPage').style.display = 'none'
                lyricsButton.classList.remove('greenButton')
            } else {
                document.getElementById('mainScreen').style.display = 'none'
                document.getElementById('lyricsPage').style.display = 'block'
                lyricsButton.classList.add('greenButton')
            }
        }
    </script>
